function create_lc(story_id, pos, lv, gv, dlv, dgv, dpos, ddir, lvl_name, silent, radius, quiet)
	local obj = create_level_changer(story_id, pos, lv, gv, dlv, dgv, dpos, ddir, lvl_name, silent, radius)
	if not quiet then
		sms("create_lc_0", nil, nil, 10000)
	end
	return obj
end


